{
  "name": "angular.js-server-test-client",
  "author": "Antoine LUCAS <antoine.lucas.australia@gmail.com>",
  "scripts": {
    "bower": "./node_modules/.bin/bower install",
    "build": "./node_modules/.bin/webpack && ./node_modules/.bin/webpack --config webpack.config.ng-cache.js",
    "test": "./node_modules/.bin/karma start test/karma-unit.conf.js --single-run && ./node_modules/.bin/karma start test/karma-unit.ng-cache.conf.js --single-run",
    "codecov-default": "./node_modules/.bin/codecov --file ./coverage/default/lcov.info",
    "codecov-ng-cache": "./node_modules/.bin/codecov --file ./coverage/ng-cache/lcov.info"
  },
  "pre-commit": [
    "bower",
    "build",
    "test"
  ],
  "main": "dist/app.js",
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-lucas/angular.js-server-test-client.git"
  },
  "engines": {
    "node": ">=5.12.0",
    "npm": ">=3.8.6"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-webpack": "^6.4.3",
    "babel-preset-stage-0": "^6.5.0",
    "bower": "^1.7.9",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "coveralls": "^2.11.12",
    "express": "^4.14.0",
    "karma": "latest",
    "karma-chai": "latest",
    "karma-chrome-launcher": "latest",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "latest",
    "karma-html2js-preprocessor": "latest",
    "karma-json-preprocessor": "latest",
    "karma-mocha": "latest",
    "karma-ng-scenario": "latest",
    "karma-requirejs": "latest",
    "karma-script-launcher": "latest",
    "mocha": "latest",
    "pre-commit": "^1.1.3",
    "requirejs": "^2.3.2",
    "webpack": "^1.13.2"
  }
}
